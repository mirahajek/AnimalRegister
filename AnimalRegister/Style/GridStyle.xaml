<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:AnimalRegister.Style">


    <Style x:Key="HeaderGrid" TargetType="Grid">
        <Setter Property="Background" Value="White"/>
    </Style>

    <Style x:Key="Light" TargetType="Grid">
        <Setter Property="Background" Value="#FFEFF1FF" />
    </Style>

    <Style x:Key="Dark" TargetType="Grid">
        <Setter Property="Background" Value="#FF474B65" />
    </Style>

    <Style x:Key="Medium" TargetType="Grid">
        <Setter Property="Background" Value="#FF6F79B6" />
    </Style>
        
     
</ResourceDictionary>